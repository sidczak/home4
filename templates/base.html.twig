<!DOCTYPE html>
<html class="h-100">
    <head>
        <meta charset="UTF-8">
        <meta http-equiv="x-ua-compatible" content="ie=edge">
        <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no, minimal-ui">
        <title>{% block title %}Welcome!{% endblock %}</title>
        {% block stylesheets %}{% endblock %}
        <link rel="stylesheet" href="{{ asset('build/js/home.css') }}" />
    </head>
    <body class="d-flex flex-column h-100 {{ app.request.get('_route') in  ['examples_banner'] ? 'navbar-banner-space' }}">
        <div id="app" class="d-none"></div>
        {% block navigation %}
            {{ include('_navbar.html.twig') }}
        {% endblock %}
        {% block body_bs %}{% endblock %}
        {% block body_sf %}{% endblock %}
        {% block body_vue %}{% endblock %}
        {% block body %}{% endblock %}
        {% block footer %}
            {{ include('_footer.html.twig') }}
        {% endblock %}
        {% block javascripts %}{% endblock %}
        {% block javascriptsvue %}
            <script src="{{ asset('build/js/home.js') }}"></script>
        {% endblock %}
    </body>
</html>
