{% extends 'base.html.twig' %}
{% block title %}Start{% endblock %}
{% block stylesheets %}{% endblock %}

{% block navigation %}
    {{ parent() }}
    {{ include('_navbarDashboard.html.twig') }}
{% endblock %}

{% block body %}
<section class="d-flex flex-shrink-0 section-min-h-100">
    <div class="container-fluid">
        <div class="row section-min-h-100">
            {% block sidebar %}
                {{ include('_sidebarDashboard.html.twig') }}
            {% endblock %}
            <div class="col col-lg-9 p-0">
                {% block content %}{% endblock %}
            </div>
        </div>
    </div>
</section>
{% endblock %}

{% block javascripts %}{% endblock %}