{% extends 'bootstrap/layout.html.twig' %}
{% block title %}Buttons{% endblock %}
{% block stylesheets %}{% endblock %}

{% block content_bs %}
    <div class="col p-0">
        <section class="d-flex flex-shrink-0 section-py-80 section-min-h-100">
            <div class="container-fluid">
                <div class="row section-min-h-100">
                    <div class="col-lg-12 d-flex justify-content-center align-items-center">
                        <div class="text-center text-white">

                            <div class="row mb-5">
                                <div class="col-12">
                                    <button type="button" class="btn btn-primary">Primary</button>
                                    <button type="button" class="btn btn-secondary">Secondary</button>
                                    <button type="button" class="btn btn-success">Success</button>
                                    <button type="button" class="btn btn-danger">Danger</button>
                                    <button type="button" class="btn btn-warning">Warning</button>
                                    <button type="button" class="btn btn-info">Info</button>
                                    <button type="button" class="btn btn-light">Light</button>
                                    <button type="button" class="btn btn-dark">Dark</button>
                                </div>
                            </div>
                            <div class="row  mb-5">
                                <div class="col-12">
                                    <button type="button" class="btn btn-outline-primary">Primary</button>
                                    <button type="button" class="btn btn-outline-secondary">Secondary</button>
                                    <button type="button" class="btn btn-outline-success">Success</button>
                                    <button type="button" class="btn btn-outline-danger">Danger</button>
                                    <button type="button" class="btn btn-outline-warning">Warning</button>
                                    <button type="button" class="btn btn-outline-info">Info</button>
                                    <button type="button" class="btn btn-outline-light">Light</button>
                                    <button type="button" class="btn btn-outline-dark">Dark</button>
                                </div>
                            </div>

                            <div class="row mb-5">
                                <div class="col-12 mb-2">
                                    <button type="button" class="btn btn-pill btn-peter-river btn-lg">Button</button>
                                    <button type="button" class="btn btn-pill btn-peter-river">Button</button>
                                    <button type="button" class="btn btn-pill btn-peter-river btn-sm">Button</button>
                                    <button type="button" class="btn btn-pill btn-outline-peter-river btn-lg">Button</button>
                                    <button type="button" class="btn btn-pill btn-outline-peter-river">Button</button>
                                    <button type="button" class="btn btn-pill btn-outline-peter-river btn-sm">Button</button>
                                </div>
                                <div class="col-12 mb-2">
                                    <button type="button" class="btn btn-pill btn-turquoise btn-lg">Button</button>
                                    <button type="button" class="btn btn-pill btn-turquoise">Button</button>
                                    <button type="button" class="btn btn-pill btn-turquoise btn-sm">Button</button>
                                    <button type="button" class="btn btn-pill btn-outline-turquoise btn-lg">Button</button>
                                    <button type="button" class="btn btn-pill btn-outline-turquoise">Button</button>
                                    <button type="button" class="btn btn-pill btn-outline-turquoise btn-sm">Button</button>
                                </div>
                                <div class="col-12 mb-2">
                                    <button type="button" class="btn btn-pill btn-emerald btn-lg">Button</button>
                                    <button type="button" class="btn btn-pill btn-emerald">Button</button>
                                    <button type="button" class="btn btn-pill btn-emerald btn-sm">Button</button>
                                    <button type="button" class="btn btn-pill btn-outline-emerald btn-lg">Button</button>
                                    <button type="button" class="btn btn-pill btn-outline-emerald">Button</button>
                                    <button type="button" class="btn btn-pill btn-outline-emerald btn-sm">Button</button>
                                </div>
                                <div class="col-12 mb-2">
                                    <button type="button" class="btn btn-pill btn-wet-asphalt btn-lg">Button</button>
                                    <button type="button" class="btn btn-pill btn-wet-asphalt">Button</button>
                                    <button type="button" class="btn btn-pill btn-wet-asphalt btn-sm">Button</button>
                                    <button type="button" class="btn btn-pill btn-outline-wet-asphalt btn-lg">Button</button>
                                    <button type="button" class="btn btn-pill btn-outline-wet-asphalt">Button</button>
                                    <button type="button" class="btn btn-pill btn-outline-wet-asphalt btn-sm">Button</button>
                                </div>
                                <div class="col-12 mb-2">
                                    <button type="button" class="btn btn-pill btn-midnight-blue btn-lg">Button</button>
                                    <button type="button" class="btn btn-pill btn-midnight-blue">Button</button>
                                    <button type="button" class="btn btn-pill btn-midnight-blue btn-sm">Button</button>
                                    <button type="button" class="btn btn-pill btn-outline-midnight-blue btn-lg btn-bw-1 btn-px-2">Button</button>
                                    <button type="button" class="btn btn-pill btn-outline-midnight-blue">Button</button>
                                    <button type="button" class="btn btn-pill btn-outline-midnight-blue btn-sm">Button</button>
                                </div>
                            </div>

                            <div class="row">
                                <div class="col-12 mb-1">
                                    {% for val in [50, 100, 200, 300, 400, 500, 600, 700, 800, 900] %}
                                        <button type="button" class="btn btn-red-{{val}}">Button {{val}}</button>
                                    {% endfor %}
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-12 mb-1">
                                    {% for val in [50, 100, 200, 300, 400, 500, 600, 700, 800, 900] %}
                                        <button type="button" class="btn btn-pink-{{val}}">Button {{val}}</button>
                                    {% endfor %}
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-12 mb-1">
                                    {% for val in [50, 100, 200, 300, 400, 500, 600, 700, 800, 900] %}
                                        <button type="button" class="btn btn-silver-{{val}}">Button {{val}}</button>
                                    {% endfor %}
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-12 mb-1">
                                    {% for val in [50, 100, 200, 300, 400, 500, 600, 700, 800, 900] %}
                                        <button type="button" class="btn btn-peter-river-{{val}}">Button {{val}}</button>
                                    {% endfor %}
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-12 mb-1">
                                    {% for val in [50, 100, 200, 300, 400, 500, 600, 700, 800, 900] %}
                                        <button type="button" class="btn btn-turquoise-{{val}}">Button {{val}}</button>
                                    {% endfor %}
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-12 mb-1">
                                    {% for val in [50, 100, 200, 300, 400, 500, 600, 700, 800, 900] %}
                                        <button type="button" class="btn btn-emerald-{{val}}">Button {{val}}</button>
                                    {% endfor %}
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-12 mb-1">
                                    {% for val in [50, 100, 200, 300, 400, 500, 600, 700, 800, 900] %}
                                        <button type="button" class="btn btn-wet-asphalt-{{val}}">Button {{val}}</button>
                                    {% endfor %}
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-12 mb-1">
                                    {% for val in [50, 100, 200, 300, 400, 500, 600, 700, 800, 900] %}
                                        <button type="button" class="btn btn-midnight-blue-{{val}}">Button {{val}}</button>
                                    {% endfor %}
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </section>
    </div>
{% endblock %}

{% block javascripts %}{% endblock %}